"1.0b3 (created: 2010/11/06 12:31:35)

highlight CmdLine font-family: monospace !important; padding: 1px !important; font-size:.95em;
set runtimepath=/home/prvak/.vimperator
set wildmode=list:full
" list:longest

" buftabs?

" hi -a TabClose display:none !important;

map e :write<CR>
map E :write<CR>:tabclose<CR>
noremap < :tabprevious<CR>
noremap > :tabnext<CR>
noremap , :tabprevious<CR>
noremap . :tabnext<CR>
noremap <C-l> :nohlsearch<CR>

set hintchars=fjghdkslaqwertyuiopzxcvbnm
set newtab=all

" complete search engine suggestions, too
set complete=slfS

" set suggestengines="google"

highlight! Hint font-size: 15px; color: black; font-weight: bold; border: 1px solid black; background-color: #fff; text-transform: uppercase; padding: 3px; font-family: Terminus, monospace;
highlight! HintElem background-color: transparent
highlight! HintActive background-color: transparent
