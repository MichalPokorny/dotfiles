Config { font = "-misc-fixed-*-*-*-*-9-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top
       , lowerOnStart = False 
       , commands = [
                    Run Cpu ["-L","3","-H","50","--normal","green","--high","red","-t","<total>"] 10
                    , Run Battery ["-t","<left>"] 10
                    , Run Com "date" ["'+%H:%M:%S %a %e.%m'"] "datum" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%cpu% | %battery% | %StdinReader% }{ <fc=#ee9a00>%datum%</fc>"
       }
